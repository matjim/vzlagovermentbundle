{% extends '::base.html.twig' %}

{% trans_default_domain 'TecnocreacionesVzlaGovernmentBundle' %}

{% block stylesheets %}
    {% stylesheets 
            'bundles/tecnocreacionesvzlagovernment/template/developer/css/styles/switches3860.css'

            filter='uglifycss' filter='cssrewrite'
            output='compiled/stylesheets_form_sigtec.min.css'
    %}
         <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    <!-- jQuery Form Validation -->
    <link rel="stylesheet" href="{{ asset('bundles/tecnocreacionesvzlagovernment/template/developer/js/libs/formValidator/developr.validationEngine3860.css?v=1') }}">
{% endblock %}

{% block javascript %}
    {{ parent() }}
    <script type="text/javascript">
        $(document).ready(function(){
            // Form validation
            $('form').validationEngine({showOneMessage:true, binded: true,autoHidePrompt:true,autoHideDelay:1500, scroll: false});
        });
        $.fn.styleSelect.defaults.allValuesText = '{{ 'tecnocreaciones_vzla_government.all'|trans }}';
        $.fn.styleSelect.defaults.multipleValuesText = '%d {{ 'tecnocreaciones_vzla_government.selected'|trans }}';
        $.fn.styleSelect.defaults.searchText = '{{ 'tecnocreaciones_vzla_government.search'|trans }}';
    </script>
{% endblock %}
{% block javascripts %}
    {{ parent() }}
    <!-- jQuery Form Validation -->
    <script src="{{ asset('bundles/tecnocreacionesvzlagovernment/template/developer/js/libs/formValidator/jquery.validationEngine3860.js?v=1') }}"></script>
    <script src="{{ asset('bundles/tecnocreacionesvzlagovernment/template/developer/js/libs/formValidator/languages/jquery.validationEngine-' ~ app.request.locale ~ '3860.js?v=1') }}"></script>
{% endblock javascripts %}