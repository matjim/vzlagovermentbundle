        var modalOpen = null,modalConfirm = null;
        function getModalOpen(){
                if(modalOpen === null){
                    modalOpen = $("#dialog-form");
                    modalOpen.dialog({
                                    autoOpen: false,
                                    height: 700,
                                    width: "90%",
                                    modal: true,
                            buttons: {
                              "Add": function(){modalOpen.dialog( "close" );},
                              Cancel: function() {
                                modalOpen.dialog( "close" );
                              }
                            },
                            close: function() {
                              //form[ 0 ].reset();
                              //allFields.removeClass( "ui-state-error" );
                            }
                      });
                }
                 return modalOpen;
            }
            
            function getModalConfirm(){
                if(modalConfirm === null){
                    modalConfirm = $( "#dialog-confirm" ).dialog({
                        autoOpen: false,
                        resizable: false,
                        height:200,
                        modal: true,
                        buttons: {
                          Si: function() {
                            jQuery( this ).dialog( "close" );
                          },
                          No: function() {
                            jQuery( this ).dialog( "close" );
                          }
                        }
                      });
                }
                return modalConfirm;
            }
        
